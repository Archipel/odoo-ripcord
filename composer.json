{
    "name": "tbondois/odoo-ripcord",
    "description": "Ripoo : a PHP Client for Odoo using Ripcord XML-RPC library, as used in Odoo Web API docs",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "darkaonline/ripcord": "^0.1.6"
    },
    "require-dev": {
        "symfony/var-dumper": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "Ripoo\\": "src/"
        }
    },
    "license": "MIT",
    "homepage": "https://packagist.org/packages/tbondois/odoo-ripcord",
    "authors": [
        {
            "name": "Thomas Bondois"
        }
    ],
    "keywords": [
        "Odoo",
        "API",
        "RPC",
        "XML",
        "xmlrpc",
        "ripcord",
        "Magento2",
        "XML-RPC"
    ]
}
